{
  "name": "@typed-decorators/autobind",
  "version": "0.2.0",
  "description": "A DOTADIW decorator for TypeScript to bind `this` context",
  "main": "dist/autobind.js",
  "types": "dist/autobnd.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "ts-node test/*.test.ts",
    "format": "npx prettier --write example/*.ts src/*.ts test/*.ts",
    "lint": "npx tslint --project tsconfig.json"
  },
  "keywords": [
    "decorators",
    "bind"
  ],
  "contributors": [
    {
      "name": "FUJI Goro",
      "url": "https://github.com/gfx"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/typed-decorators/autobind"
  },
  "license": "ISC",
  "devDependencies": {
    "ts-node": ">= 5.0.1",
    "tslint": "*",
    "tslint-config-prettier": "*",
    "typescript": ">= 2.8.1"
  },
  "dependencies": {
    "tslib": "*"
  },
  "sideEffects": false
}
